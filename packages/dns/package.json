{
  "name": "@beatrice/dns",
  "version": "1.3.0",
  "description": "DNS",
  "author": "Eyzi <contact@eyzi.xyz>",
  "homepage": "https://github.com/eyzi/beatrice/tree/main/packages/dns#readme",
  "private": true,
  "license": "ISC",
  "main": "lib/index.js",
  "directories": {
    "lib": "lib",
    "test": "__tests__"
  },
  "files": [
    "lib"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/eyzi/beatrice.git"
  },
  "scripts": {
    "build": "npx -p typescript tsc",
    "build:clean": "yarn clean && yarn build",
    "clean": "rm -rf lib && rm -rf node_modules && rm -rf tsconfig.tsbuildinfo",
    "deploy": "VERSION=$npm_package_version docker-compose up --build -d",
    "deploy:down": "docker-compose down",
    "lerna:link": "npx lerna link",
    "start": "node .",
    "start:dev": "npx ts-node src/index.ts",
    "start:dev:build": "yarn build && ts-node src/index.ts",
    "test": "npx nyc mocha"
  },
  "bugs": {
    "url": "https://github.com/eyzi/beatrice/issues"
  },
  "nyc": {
    "check-coverage": true,
    "extension": [
      ".ts"
    ],
    "include": [
      "src/**/*.ts"
    ],
    "lines": 100,
    "statements": 100,
    "functions": 100,
    "branches": 100,
    "per-file": true
  },
  "mocha": {
    "require": [
      "ts-node/register"
    ],
    "extension": [
      "ts"
    ],
    "spec": "__tests__/**/*.test.ts"
  },
  "devDependencies": {
    "@types/chai": "^4.2.21",
    "@types/express": "^4.17.13",
    "@types/mocha": "^9.0.0",
    "@types/mongoose": "^5.11.97",
    "@types/node": "^16.4.13",
    "@types/sinon": "^10.0.2",
    "chai": "^4.3.4",
    "mocha": "^9.0.3",
    "nyc": "^15.1.0",
    "sinon": "^11.1.2"
  },
  "dependencies": {
    "@beatrice/common": "^1.1.6",
    "dnsd": "^0.9.6",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "mongoose": "^5.13.7",
    "public-ip": "^4.0.4"
  }
}
