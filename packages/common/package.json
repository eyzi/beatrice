{
  "name": "@beatrice/common",
  "version": "1.1.3",
  "description": "> TODO: description",
  "author": "Eyzi <contact@eyzi.xyz>",
  "homepage": "https://github.com/eyzi/beatrice/tree/main/packages/common#readme",
  "private": true,
  "license": "UNLICENSED",
  "main": "lib/index.js",
  "directories": {
    "lib": "lib",
    "test": "__tests__"
  },
  "files": [
    "lib"
  ],
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/eyzi/beatrice.git"
  },
  "scripts": {
    "build:common": "npx -p typescript tsc",
    "build:clean": "yarn clean && yarn build",
    "clean": "rm -rf lib && rm -rf node_modules && rm -rf tsconfig.tsbuildinfo",
    "lerna:link": "npx lerna link",
    "test": "npx nyc mocha"
  },
  "bugs": {
    "url": "https://github.com/eyzi/beatrice/issues"
  },
  "nyc": {
    "check-coverage": true,
    "extension": [
      ".ts"
    ],
    "include": [
      "src/**/*.ts"
    ],
    "lines": 100,
    "statements": 100,
    "functions": 100,
    "branches": 100,
    "per-file": true
  },
  "mocha": {
    "require": [
      "ts-node/register"
    ],
    "extension": [
      "ts"
    ],
    "spec": "__tests__/**/*.test.ts"
  },
  "devDependencies": {
    "@types/chai": "^4.2.21",
    "@types/mocha": "^9.0.0",
    "@types/mongoose": "^5.11.97",
    "@types/node": "^16.6.1",
    "@types/sinon": "^10.0.2",
    "chai": "^4.3.4",
    "mocha": "^9.0.3",
    "nyc": "^15.1.0",
    "sinon": "^11.1.2"
  },
  "dependencies": {
    "dotenv": "^10.0.0",
    "mongoose": "^5.13.7"
  }
}
